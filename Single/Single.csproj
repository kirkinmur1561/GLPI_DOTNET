<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0</Version>
        <Title>Lib for GLPI RESTAPI</Title>
        <Description>see to https://github.com/kirkinmur1561/GLPI_DOTNET#glpi-api</Description>
        <PackageProjectUrl>https://github.com/kirkinmur1561/GLPI_DOTNET</PackageProjectUrl>
        <PackageTags>GLPI; GLPI_API; GLPI_REST_API</PackageTags>
        <PackageId>GlpiRestApi</PackageId>
        <Product>GlpiRestApi</Product>
        <AssemblyVersion>1.0.0</AssemblyVersion>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\CommonObj\CommonObj.csproj" />
    </ItemGroup>

</Project>
